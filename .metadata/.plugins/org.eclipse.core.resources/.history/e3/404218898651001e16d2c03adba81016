package proassign2;
import java.util.Scanner;
public class demo11 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int sum=0;
		System.out.println("enter the natural number");
		int a=sc.nextInt();
		
		for (int i=1;i<=a;i++)
		{
			sum=sum+i;
			System.out.println(sum);
		}
		

	}

}
